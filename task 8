#include <stdio.h>

/*1)Направете побитовите операции върху целите числа 3 и 5 в main().
Покажете резултата с функцията printf(“%d”, res);*/


    int main()
    {
        char a=1, b=2, res;

    //a AND b
    res=a&b;
    printf("3 and 5 is: %d\n", res);

    //a OR b;
    res=a|b;
    printf("3 or 5 is: %d\n", res);

    //a XOR b
    printf("3 XOR 5 is: %d\n", res);

    return 0;
        
    }
